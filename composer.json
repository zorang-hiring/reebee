{
    "name": "zoran/reebee",
    "type": "project",
    "authors": [
        {
            "name": "Zoran Gasic",
            "email": "zoran.gasic@cleoo.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "require": {},
    "require-dev": {
        "phpunit/phpunit": "^7",
        "spatie/phpunit-watcher": "^1.23"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit tests",
        "test:watch": [
            "Composer\\Config::disableProcessTimeout",
            "./vendor/bin/phpunit-watcher watch < /dev/tty"
        ]
    }
}
